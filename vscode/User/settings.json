{
    "vim.useCtrlKeys": false,
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "k"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.visualModeKeyBindings": [
        {
            "before": [
                "f"
            ],
            "after": [
                "y"
            ]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "f",
                "f"
            ],
            "after": [
                "y",
                "y"
            ]
        }
    ],
    "vim.leader": ",",
    "files.autoSave": "afterDelay",
    "explorer.confirmDragAndDrop": false,
    "editor.wordWrap": "on",
    "python.condaPath": "/home/geri/anaconda3/bin/conda",
    "python.venvPath": "~/anaconda3/envs",
    "editor.multiCursorModifier": "ctrlCmd",
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/*.o": true,
        "**/node_modules/**": true,
        "**/target/**": true,
        "build": true
    },
    "editor.renderIndentGuides": false,
    "C_Cpp.clang_format_style": "none",
    "C_Cpp.clang_format_fallbackStyle": "none",
    "editor.mouseWheelZoom": true,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "python.jediEnabled": true,
    "files.exclude": {
        ".GOPATH": true,
        "**/*.a": true,
        "**/*.o": true,
        "build": true
    },
    "search.exclude": {
        "**/*.o": true,
        "build/**": true
    },
    "workbench.tree.indent": 25,
    "go.inferGopath": true,
    "editor.formatOnSave": true,
    "vs-kubernetes": {
        "vs-kubernetes.helm-path": "/home/greg/.vs-kubernetes/tools/helm/linux-amd64/helm",
        "vs-kubernetes.minikube-path": "/home/greg/.vs-kubernetes/tools/minikube/linux-amd64/minikube",
        "vs-kubernetes.draft-path": "/home/greg/.vs-kubernetes/tools/draft/linux-amd64/draft"
    },
    "[python]": {},
    "yaml.validate": false,
    "yaml.format.enable": false,
    "python.linting.enabled": false,
    "python.terminal.activateEnvironment": false,
    "editor.minimap.maxColumn": 1200,
    "editor.minimap.enabled": false,
    "workbench.colorTheme": "Solarized Light",
    "editor.cursorBlinking": "solid",
    "C_Cpp.updateChannel": "Insiders",
    "window.zoomLevel": 0,
    "files.associations": {
        "*.html": "html"
    },
    "sqltools.useNodeRuntime": true,
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
}