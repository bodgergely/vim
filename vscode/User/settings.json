{
    "vim.useCtrlKeys": false,
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "k"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.normalModeKeyBindings": [
        {
            "before": [
                "f"
            ],
            "after": [
                "y"
            ]
        },
        {
            "before": [
                "v",
                "v"
            ],
            "after": [
                "0"
            ]
        },
        {
            "before": [
                "n",
                "n"
            ],
            "after": [
                "<S>",
                "4"
            ]
        }
    ],
    "editor.minimap.enabled": false,
    "vim.leader": ",",
    "rust.mode": "rls",
    "rust.rustup": {
        "toolchain": "stable-x86_64-unknown-linux-gnu",
        "nightlyToolchain": "nightly-x86_64-unknown-linux-gnu"
    },
    "files.autoSave": "afterDelay",
    "explorer.confirmDragAndDrop": false,
    "editor.wordWrap": "on",
    "python.condaPath": "/home/geri/anaconda3/bin/conda",
    "python.venvPath": "~/anaconda3/envs",
    "editor.multiCursorModifier": "ctrlCmd",
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/*.o": true,
        "**/node_modules/**": true,
        "**/target/**": true,
        "build": true
    },
    "editor.renderIndentGuides": false,
    "C_Cpp.clang_format_style": "WebKit",
    "C_Cpp.clang_format_fallbackStyle": "WebKit",
    "editor.mouseWheelZoom": true,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "python.jediEnabled": true,
    "debug.console.fontSize": 16,
    "window.zoomLevel": -1,
    "files.exclude": {
        ".GOPATH": true,
        "**/*.a": true,
        "**/*.o": true,
        "build": true
    },
    "search.exclude": {
        "**/*.o": true,
        "build/**": true
    },
    "workbench.tree.indent": 20,
    "rust.rls": {
        "useRustfmt": false
    },
    "go.inferGopath": true,
    "editor.formatOnSave": true,
    "go.lintTool": "golangci-lint",
    "vs-kubernetes": {
        "vs-kubernetes.helm-path": "/home/greg/.vs-kubernetes/tools/helm/linux-amd64/helm",
        "vs-kubernetes.minikube-path": "/home/greg/.vs-kubernetes/tools/minikube/linux-amd64/minikube",
        "vs-kubernetes.draft-path": "/home/greg/.vs-kubernetes/tools/draft/linux-amd64/draft"
    },
    "workbench.colorTheme": "Default Light+",
}